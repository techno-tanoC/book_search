{
  "manifest_version": 2,
  "name": "book search",
  "description": "bookmark searcher",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "book search",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
